public class Main
{
	public static void main(String[] args) {
	   long a = 1;
	   long b = 2;
	   long iterator = a+b;
	   long sum = 2;
	   while(iterator < 4000000)
	    {
	        if(iterator %2 == 0)
	            sum += iterator;
	        
	        a = b;
	        b = iterator;
	        iterator = a + b;
	    }
	    System.out.println("Sum of the even-valued terms in fibonacci series is:" + sum);
	}
}
